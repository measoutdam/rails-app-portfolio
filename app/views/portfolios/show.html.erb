<%= link_to "Edit", edit_portfolio_path(@portfolio.id) %>
<p>
<%= image_tag @portfolio.main_image unless @portfolio.main_image.nil? %>
<h1>
  <%= @portfolio.title %>
</h1>
<em>
  <%= @portfolio.subtitle %>
</em>
<p>
  <%= @portfolio.body %>
</p>
<p>
  <h3>Technologies used:</h3>
  <ul>
  <% @portfolio.technologies.each do |t|  %>
    <li><%= t.name  %></li>
  <% end %>
  </ul>
</p>
<hr>
<%= params.inspect %>
<hr>
